&lt;span class="kr"&gt;import&lt;/span&gt; &lt;span class="nx"&gt;GamesList&lt;/span&gt; &lt;span class="nx"&gt;from&lt;/span&gt; &lt;span class="s1"&gt;'./GamesList'&lt;/span&gt;&lt;span class="p"&gt;;&lt;/span&gt;
